    "word",
    "sketches",
    "word",
    "combinations",
    "word",
    "differences",
    "animation",
    "words"
  ],
  "author": {
    "name": "Lucia Kocincova",
    "email": "ping@lucyia.com",
    "url": "lucyia.com"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "d3": "^4.10.0",
    "d3-tip": "0.8.0-alpha.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "ske-viz": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
